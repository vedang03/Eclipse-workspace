package passbyvalue;


public class PassByValue {
	
	static int incrementNumberByTen(int number) {
		  number+=10;
		  return number
	}



	public static void main(String[] args) {
		int number = 1;
		System.out.println(number);
		incrementNumberByTen(number);
		System.out.println(number);
		
		

	}
	

}
