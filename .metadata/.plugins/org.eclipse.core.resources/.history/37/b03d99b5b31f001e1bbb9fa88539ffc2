package com.aurionpro.test;

import com.aurionpro.model.Color;

import com.aurionpro.model.Rectangle;
import java.util.Scanner;

public class RectangleTest {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		Rectangle r1 = new Rectangle();
        r1.setHeight(sc.nextDouble());
        r1.setWidth(sc.nextDouble());
        
       
        
        
        try {
        	r1.setColour(Color.valueOf(sc.next().toUpperCase()));
        }
        
        catch(Exception e) {
        	r1.setColour(Color.RED);
        }

		System.out.println(r1.getHeight() * r1.getWidth() + " Colour is: " + r1.getColour());
		
		 Rectangle r2 = new Rectangle();
	        r2.setHeight(sc.nextDouble());
	        r2.setWidth(sc.nextDouble());
	        
	        try {
	        	r2.setColour(Color.valueOf(sc.next().toUpperCase()));
	        }
	        
	        catch(Exception e) {
	        	r2.getColour(Color.valueOf(sc.next()));
	        }
	        
	        System.out.println(r2.getHeight() * r2.getWidth() + " Colour is: " + r2.getColour());



	}

}
