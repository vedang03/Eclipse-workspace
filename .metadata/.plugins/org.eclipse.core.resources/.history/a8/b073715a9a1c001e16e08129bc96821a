package passbyvalue;

public class IncrementByOne {

	public static void main(String[] args) {
		int[] numbers = {10,20,30,40};
		incrementNumbersByOne(numbers);
		
		for(int x : numbers) {
			System.out.println(x);
		}

	}
	
	private static void incrementNumbersByOne(int[] numbers) {
		for(int x:numbers) {
			x+=1;
		}
	}

}
